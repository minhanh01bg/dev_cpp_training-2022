main =>
  N = read_int(),
  Mark = [1 : I in 1 .. 1000],
  foreach (I in 1 .. N)
    X = read_int(),
    Y = read_int(),
    foreach (J in X .. Y)
      Mark[J] := 0
    end
  end,
  Res = 0,
  foreach (I in 1 .. 1000)
    if Mark[I] = 1 then
      Res := Res + 1
    end
  end,
  print(Res),
  foreach (I in 1 .. 1000)
    if Mark[I] = 1 then
      printf(" %d", I)
    end
  end
